     formatted = this.date.toLocaleDateString(undefined, {\n                          year: 'numeric',\n                          month: 'numeric',\n                          day: 'numeric'\n                      });\n                  }\n              }\n\n              if (this.updatedTimestamp) {\n                  formatted = 'Updated ' + formatted;\n              }\n\n              return formatted;\n          }\n      }\" x-text=\"format()\">Feb 6, 2026 7:31 PM</time>\n    <span class=\"text-gray-550\">|</span> <a class=\"view-comments text-gray-300 hover:text-gray-500\" href=\"https://arstechnica.com/space/2026/02/to-reuse-or-not-reuse-the-eternal-debate-of-new-glenns-second-stage-reignites/#comments\" title=\"65 comments\">\n    <svg class=\"-mt-1 ml-1 mr-[2px] inline-block h-4 w-4\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 80 80\"><defs><clipPath id=\"bubble-zero_svg__a\"><path fill=\"none\" stroke-width=\"0\" d=\"M0 0h80v80H0z\"></path></clipPath><clipPath id=\"bubble-zero_svg__b\"><path fill=\"none\" stroke-width=\"0\" d=\"M0 0h80v80H0z\"></path></clipPath></defs><g clip-path=\"url(#bubble-zero_svg__a)\"><g fill=\"currentColor\" clip-path=\"url(#bubble-zero_svg__b)\"><path d=\"M80 40c0 22.09-17.91 40-40 40S0 62.09 0 40 17.91 0 40 0s40 17.91 40 40\"></path><path d=\"M40 40 .59 76.58C-.67 77.84.22 80 2.01 80H40z\"></path></g></g></svg>\n    65\n  </a>\n  </span></div>\n        </div>\n          </div>\n\n    <div class=\"mt-4 min-h-1 lg:mt-0\">\n              <div class=\"relative aspect-video overflow-hidden\">\n                      <div class=\"ars-lightbox\">\n              <div class=\"ars-lightbox-item\">\n                <a class=\"cursor-zoom-in\" data-pswp-width=\"1920\" data-pswp-height=\"1074\" data-pswp-srcset=\"https://cdn.arstechnica.net/wp-content/uploads/2024/09/new-glenn_gs2-hotfire.webp 1920w, https://cdn.arstechnica.net/wp-content/uploads/2024/09/new-glenn_gs2-hotfire-300x168.webp 300w, https://cdn.arstechnica.net/wp-content/upl